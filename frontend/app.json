{
  "expo": {
    "name": "react-native-app",
    "slug": "react-native-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "package":"com.jsm.aora",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to track live updates."
      },
      "config":{
        "googleMapsApiKey": "AIzaSyDoLOsUg-0Yy_g1q9rWhjV8IeWWgE41dVM"
      }
    },
    "android": {
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package":"com.jsm.aora",
      "config" : {
        "googleMaps": {
          "apiKey": "AIzaSyDoLOsUg-0Yy_g1q9rWhjV8IeWWgE41dVM"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "package":"com.jsm.aora"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],

    

    "experiments": {
      "typedRoutes": true
    }

  }
}
